dev_not_found=device not found
dev_updated=device updated
dev_not_updated=device was not updated
dev_no_power=device does not support power states
dev_refresh=devices will be refreshed
dev_connect_failed=failed to connect to Lightify gateway
blackout=turning off all lights
dev_no_brightness=device does not support brightness
dev_no_temperatures=device does not support temperatures
dev_no_colors=device does not support colors